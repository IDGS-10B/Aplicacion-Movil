<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EmployeeRegister.Administrador">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="lblWelcome" FontSize="Medium"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" TextColor="Black"/>

            <Button Text="Registrar Empleados" Clicked="clicklogin" BackgroundColor="#6A1B9A" TextColor="White"/>

            <Button Text="Alta curso" Clicked="clickcurso" BackgroundColor="#6A1B9A" TextColor="White"/>

            <Button Text="Cerrar Sesión" Clicked="OnLogoutButtonClicked" BackgroundColor="red" TextColor="White" CornerRadius="5" />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>